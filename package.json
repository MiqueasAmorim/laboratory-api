{
  "name": "laboratory-api",
  "version": "1.0.0",
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "description": "REST API for maintenance of laboratories and exams",
  "main": "index.ts",
  "author": "Miqueas Amorim Teixeira",
  "license": "MIT",
  "scripts": {
    "prestart": "yarn run tsc",
    "start": "export NODE_ENV=PROD && node dist/index.js",
    "dev": "export NODE_ENV=DEV && tsnd --transpile-only --respawn --ignore-watch node_modules src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "16.x",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  }
}
